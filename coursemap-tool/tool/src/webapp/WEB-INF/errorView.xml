<beans>

	<bean id="org.etudes.coursemap.tool.ErrorView"
			class="org.etudes.coursemap.tool.ErrorView"
			init-method="init"
			destroy-method="destroy"
			singleton="true">

 		<property name="ui"><ref bean="org.etudes.ambrosia.api.UiService"/></property>
 
 		<property name="toolId"><value>sakai.coursemap</value></property>
 		<property name="path"><value>error</value></property>
 		<property name="bundle"><value>error</value></property>
		<property name="shared"><value>coursemap</value></property>
		<property name="components"><value>error.xml</value></property>
	</bean>

</beans>
